{
    "name": "camino",
    "description": "Flexible habit tracker",
    "license": "MIT",
    "authors": [
        "Ryan Frame"
    ],
    "copyright": "Copyright Â© 2020 Ryan Frame",
    "dependencies": {
        "sumtype": "~>0.10.2"
    },
    "configurations": [
        {
            "name": "executable",
            "targetType": "executable",
            "excludedSourceFiles": [
                "source/ut_main.d"
            ]
        },
        {
            "name": "unittest",
            "targetType": "executable",
            "dependencies": {
                "unit-threaded": "~>1.0.11"
            },
            "mainSourceFile": "source/ut_main.d",
            "excludedSourceFiles": [
                "source/main.d"
            ]
        }
    ],
    "dflags": [
        "-preview=dip25",
        "-preview=dip1000",
        "-preview=dip1008",
        "-preview=fieldwise",
        "-preview=markdown",
        "-preview=fixAliasThis",
        "-preview=dtorfields",
        "-preview=rvaluerefparam",
        "-preview=in"
    ]
}
